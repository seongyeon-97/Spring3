<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.sy.s1.bankbook.BankbookDAO">
  	
  	<delete id="setDelete" parameterType="java.lang.Long">
  		DELETE FROM BANKBOOK WHERE BOOKNUMBER=#{bookNumber}
  	</delete>
  	
  	<insert id="setInsert" parameterType="com.sy.s1.bankbook.BankbookDTO">
  		INSERT INTO BANKBOOK(BOOKNUMBER, BOOKNAME, BOOKRATE, BOOKSALE) 
  		VALUES(BANKBOOK_SEQ.NEXTVAL, #{bookName}, #{bookRate}, #{bookSale})
  	</insert>
  	
  	<select id="getList" resultType="com.sy.s1.bankbook.BankbookDTO">
  		SELECT * FROM BANKBOOK
  	</select>
  	
  	<select id="getSelect" parameterType="com.sy.s1.bankbook.BankbookDTO" resultType="com.sy.s1.bankbook.BankbookDTO">
  		SELECT * FROM BANKBOOK WHERE BOOKNUMBER=#{bookNumber}
  	</select>
  
  </mapper>